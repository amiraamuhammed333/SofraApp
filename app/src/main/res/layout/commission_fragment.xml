<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/commission_fragment_tv"
        android:text="@string/commission"
        android:textColor="@color/black"
        android:gravity="center"
        android:textSize="@dimen/dim_25sp"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="@dimen/dim_32dp"
        app:layout_constraintTop_toBottomOf="@id/commission_fragment_tv"
        android:id="@+id/commission_fragment_tv_count"
        android:textColor="@color/black"
        android:paddingStart="@dimen/dim_8dp"
        android:textSize="@dimen/dim_15sp"
        />
    <TextView
    android:layout_width="match_parent"
    android:layout_height="@dimen/dim_30dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    android:layout_marginTop="@dimen/dim_16dp"
    app:layout_constraintTop_toBottomOf="@id/commission_fragment_tv_count"
    android:id="@+id/commission_fragment_tv_commission"
    android:textColor="@color/black"
    android:paddingStart="@dimen/dim_8dp"
    android:textSize="@dimen/dim_15sp"
    />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="@dimen/dim_16dp"
        app:layout_constraintTop_toBottomOf="@id/commission_fragment_tv_commission"
        android:id="@+id/commission_fragment_tv_total"
        android:textColor="@color/black"
        android:paddingStart="@dimen/dim_8dp"
        android:textSize="@dimen/dim_15sp"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="@dimen/dim_16dp"
        app:layout_constraintTop_toBottomOf="@id/commission_fragment_tv_commission"
        android:id="@+id/commission_fragment_tv_commissions"
        android:textColor="@color/black"
        android:paddingStart="@dimen/dim_8dp"
        android:textSize="@dimen/dim_15sp"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="@dimen/dim_16dp"
        app:layout_constraintTop_toBottomOf="@id/commission_fragment_tv_commissions"
        android:id="@+id/commission_fragment_tv_payment"
        android:textColor="@color/black"
        android:paddingStart="@dimen/dim_8dp"
        android:textSize="@dimen/dim_15sp"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="@dimen/dim_16dp"
        app:layout_constraintTop_toBottomOf="@id/commission_fragment_tv_payment"
        android:id="@+id/commission_fragment_tv_netCommissions"
        android:textColor="@color/black"
        android:paddingStart="@dimen/dim_8dp"
        android:textSize="@dimen/dim_15sp"
        />

</androidx.constraintlayout.widget.ConstraintLayout>