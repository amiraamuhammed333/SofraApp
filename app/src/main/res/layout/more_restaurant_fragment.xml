<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:layout_width="@dimen/dim_200dp"
        android:layout_height="@dimen/dim_40dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="@dimen/dim_32dp"
        android:id="@+id/nore_fragment_tv"
        android:text="@string/more"
        android:textSize="@dimen/dim_25sp"
        android:gravity="center"
        android:textStyle="bold"
        />

    <TextView
        android:layout_width="@dimen/dim_200dp"
        android:layout_height="@dimen/dim_40dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="@dimen/dim_32dp"
        android:id="@+id/more_fragment_tv"
        android:text="@string/more"
        android:textColor="@color/black"
        android:textSize="@dimen/dim_25sp"
        android:gravity="center"
        android:textStyle="bold"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_40dp"
        app:layout_constraintTop_toBottomOf="@id/more_fragment_tv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="@dimen/dim_16dp"
        android:layout_marginEnd="@dimen/dim_16dp"
        android:layout_marginTop="@dimen/dim_16dp"
        android:id="@+id/more_fragment_tv_offer"
        android:text="@string/offers"
        android:textSize="@dimen/dim_25sp"
        android:textColor="@color/black"
        android:paddingStart="@dimen/dim_16dp"
        android:drawableStart="@drawable/ic_gift"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/more_fragment_tv_offer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@color/dark_grey"
        android:layout_marginStart="@dimen/dim_16dp"
        android:layout_marginEnd="@dimen/dim_16dp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_40dp"
        app:layout_constraintTop_toBottomOf="@id/more_fragment_tv_offer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="@dimen/dim_16dp"
        android:layout_marginEnd="@dimen/dim_16dp"
        android:layout_marginTop="@dimen/dim_16dp"
        android:id="@+id/more_fragment_tv_contact_us"
        android:text="@string/contact_us"
        android:textSize="@dimen/dim_25sp"
        android:textColor="@color/black"
        android:paddingStart="@dimen/dim_16dp"
        android:drawableStart="@drawable/ic_email_black"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/more_fragment_tv_contact_us"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@color/dark_grey"
        android:layout_marginStart="@dimen/dim_16dp"
        android:layout_marginEnd="@dimen/dim_16dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_40dp"
        app:layout_constraintTop_toBottomOf="@id/more_fragment_tv_contact_us"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="@dimen/dim_16dp"
        android:layout_marginEnd="@dimen/dim_16dp"
        android:layout_marginTop="@dimen/dim_16dp"
        android:id="@+id/more_fragment_tv_about_app"
        android:text="@string/about_app"
        android:textSize="@dimen/dim_25sp"
        android:textColor="@color/black"
        android:paddingStart="@dimen/dim_16dp"
        android:drawableStart="@drawable/ic_opacity"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/more_fragment_tv_about_app"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@color/dark_grey"
        android:layout_marginStart="@dimen/dim_16dp"
        android:layout_marginEnd="@dimen/dim_16dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_40dp"
        app:layout_constraintTop_toBottomOf="@id/more_fragment_tv_about_app"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="@dimen/dim_16dp"
        android:layout_marginEnd="@dimen/dim_16dp"
        android:layout_marginTop="@dimen/dim_16dp"
        android:id="@+id/more_fragment_tv_review"
        android:text="@string/review"
        android:textSize="@dimen/dim_25sp"
        android:textColor="@color/black"
        android:paddingStart="@dimen/dim_16dp"
        android:drawableStart="@drawable/ic_star_black"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/more_fragment_tv_review"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@color/dark_grey"
        android:layout_marginStart="@dimen/dim_16dp"
        android:layout_marginEnd="@dimen/dim_16dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_40dp"
        app:layout_constraintTop_toBottomOf="@id/more_fragment_tv_review"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="@dimen/dim_16dp"
        android:layout_marginEnd="@dimen/dim_16dp"
        android:layout_marginTop="@dimen/dim_16dp"
        android:id="@+id/more_fragment_tv_about_change_password"
        android:text="@string/change_password"
        android:textSize="@dimen/dim_25sp"
        android:textColor="@color/black"
        android:paddingStart="@dimen/dim_16dp"
        android:drawableStart="@drawable/ic_lock_black"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/more_fragment_tv_about_change_password"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@color/dark_grey"
        android:layout_marginStart="@dimen/dim_16dp"
        android:layout_marginEnd="@dimen/dim_16dp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_40dp"
        app:layout_constraintTop_toBottomOf="@id/more_fragment_tv_about_change_password"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="@dimen/dim_16dp"
        android:layout_marginEnd="@dimen/dim_16dp"
        android:layout_marginTop="@dimen/dim_16dp"
        android:id="@+id/more_fragment_tv_log_out"
        android:text="@string/log_out"
        android:textSize="@dimen/dim_25sp"
        android:textColor="@color/black"
        android:paddingStart="@dimen/dim_16dp"
        android:drawableStart="@drawable/ic_log_out"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dim_2dp"
        app:layout_constraintTop_toBottomOf="@id/more_fragment_tv_log_out"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@color/dark_grey"
        android:layout_marginStart="@dimen/dim_16dp"
        android:layout_marginEnd="@dimen/dim_16dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>